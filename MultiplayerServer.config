<Host>
SettingName=Количество игроков
SettingDescription=Сколько игроков будут на сервере?
SettingType=Integer
MinValue=2
MaxValue=8
Param=-host
Port=ZDoom,GZDoom
Default=2
</Host>

<Port>
SettingName=Номер порта
SettingDescription=Укажите IP-порт, к которому будут подключаться игроки.
SettingType=Integer
MinValue=1
MaxValue=65535
NotUseIfEqual=5029
Default=5029
Param=-port
Port=ZDoom,GZDoom
</Port>

<Port_ST>
SettingName=Номер порта
SettingDescription=Укажите IP-порт, к которому будут подключаться игроки.
SettingType=Integer
MinValue=0
MaxValue=65535
NotUseIfEqual=10666
Default=10666
Port=Skulltag
</Port_ST>

<GameType_ST>
SettingName=Режим игры
SettingDescription=Выберите режим игры.
SettingType=List
Item0=Deathmatch
Value0=+deathmatch 1
Item1=TeamDeathMatch
Value1=+deathmatch 1 +teamplay 1
Item2=Cooperative
Value2=+cooperative 1
Item3=Survival
Value3=+survival 1
Item4=Invasion
Value4=+invasion 1
Item5=CTF
Value5=+ctf 1
Item6=OneFlagCTF
Value6=+oneflagctf 1
Item7=Possession
Value7=+possession 1
Item8=TeamPossession
Value8=+teampossession 1
Item9=Terminator
Value9=+terminator 1
Item10=Domination
Value10=+domination 1
Item11=Skulltag
Value11=+skulltag 1
Item12=Duel
Value12=+duel 1
Item13=Last Man Standing
Value13=+lastmanstanding 1
Item14=Team Last Man Standing
Value14=+teamlms 1
Port=Skulltag
Default=+deathmatch 1
</GameType_ST>

<Modifier>
SettingName=Модификатор
SettingDescription=Применить дополнительный модификатор к игре
SettingType=List
Item0=Instagib
Value0=+instagib 1
Item1=Buckghot
Value1=+buckshot 1
AddEmptyItem=True
Port=Skulltag
</Modifier>

<MaxPlayers_ST>
SettingName=Лимит игроков
SettingDescription=Сколько игроков одновременно смогут играть на сервере?
SettingType=Integer
MinValue=1
MaxValue=8
Param=+sv_maxplayers
Port=Skulltag
Default=4
</MaxPlayers_ST>

<MaxClients_ST>
SettingName=Лимит клиентов
SettingDescription=Сколько игроков одновременно смогут подключиться к серверу?
SettingType=Integer
MinValue=1
MaxValue=32
Param=+sv_maxclients
Port=Skulltag
Default=4
</MaxClients_ST>

<Fraglimit_ST>
SettingName=Лимит фрагов
SettingDescription=Количество фрагов, необходимое для выигрыша. 0 - игнорируется
SettingType=Integer
MinValue=0
MaxValue=999
Param=+fraglimit
//NotUseIfEqual=0
Default=0
DependsOn0=GameType_ST
DependValue0=+deathmatch 1
DependCondition0=Equal
DependsOn1=GameType_ST
DependValue1=+deathmatch 1 +teamplay 1
DependCondition1=Equal
DependsOn2=GameType_ST
DependValue2=+duel 1
DependCondition2=Equal
DependsOn3=GameType_ST
DependValue3=+terminator 1
DependCondition3=Equal
DependMode=OR
Port=Skulltag
</Fraglimit_ST>

<Fraglimit>
SettingName=Лимит фрагов
SettingDescription=Количество фрагов, необходимое для выигрыша. 0 - игнорируется
SettingType=Integer
MinValue=0
MaxValue=999
Param=+fraglimit
Default=0
DependsOn0=Deathmatch
DependCondition0=Equal
DependValue0=True
Port=ZDoom,GZDoom
</Fraglimit>

<Timelimit>
SettingName=Лимит времени
SettingDescription=Количество минут, по истечении которых игра заканчивается. 0 - игнорируется
SettingType=Integer
MinValue=0
MaxValue=999
Param=+timelimit
//NotUseIfEqual=0
Default=0
</Timelimit>

<Pointlimit>
SettingName=Лимит очков
SettingDescription=Количество очков, необходимое для выигрыша. 0 - игнорируется
SettingType=Integer
MinValue=0
MaxValue=999
Param=+pointlimit
Port=Skulltag
//NotUseIfEqual=0
Default=0
DependsOn0=GameType_ST
DependValue0=+ctf 1
DependCondition0=Equal
DependsOn1=GameType_ST
DependValue1=+oneflagctf 1
DependCondition1=Equal
DependsOn2=GameType_ST
DependValue2=+possession 1
DependCondition2=Equal
DependsOn3=GameType_ST
DependValue3=+teampossession 1
DependCondition3=Equal
DependsOn4=GameType_ST
DependValue4=+skulltag 1
DependCondition4=Equal
DependMode=OR
</Pointlimit>

<Duellimit>
SettingName=Лимит дуэлей
SettingDescription=Максимальное количество дуэлей, играемых на одной карте
SettingType=Integer
MinValue=0
MaxValue=999
Param=+duellimit
Port=Skulltag
//NotUseIfEqual=0
Default=0
DependsOn0=GameType_ST
DependValue0=+duel 1
DependCondition0=Equal
</Duellimit>

<Winlimit>
SettingName=Лимит выигрышей
SettingDescription=Сколько дуэлей или раундов (Team)LMS необходимо выиграть для победы. 0 - игнорируется
SettingType=Integer
MinValue=0
MaxValue=999
Param=+winlimit
Port=Skulltag
//NotUseIfEqual=0
Default=0
DependsOn0=GameType_ST
DependValue0=+duel 1
DependCondition0=Equal
DependsOn1=GameType_ST
DependValue1=+lastmanstanding 1
DependCondition1=Equal
DependsOn2=GameType_ST
DependValue2=+teamlms 1
DependCondition2=Equal
DependMode=OR
</Winlimit>

<MaxLives>
SettingName=Количество жизней
SettingDescription=Сколько раз за раунд можно респаунится. По исчерпании лимита игрок становится наблюдателем и не может присоединиться до конца раунда.
SettingType=Integer
MinValue=0
MaxValue=999
Port=Skulltag
//NotUseIfEqual=0
Default=0
DependsOn0=GameType_ST
DependValue0=+invasion 1
DependCondition0=Equal
DependsOn1=GameType_ST
DependValue1=+survival 1
DependCondition1=Equal
DependMode=OR
</MaxLives>

<Deathmatch>
SettingName=Deathmatch
SettingDescription=Включить deathmatch-режим
SettingType=Boolean
Param=+deathmatch 1
Port=ZDoom,GZDoom
NotUseIfEqual=False
</Deathmatch>

<TeamPlay>
SettingName=Командный режим
SettingDescription=Включить командный режим. Действительно, только если включен deathmatch.
SettingType=Boolean
Param=+teamplay 1
Port=ZDoom,GZDoom
NotUseIfEqual=False
DependsOn0=Deathmatch
DependValue0=True
DependCondition0=Equal
</TeamPlay>

<DebugFile>
SettingName=Запись отладочного файла
SettingDescription=Запись отладочных сообщений в файл debugN.txt, где N - номер игрока.
SettingType=Boolean
Param=-debugfile
Port=ZDoom,GZDoom
</DebugFile>

<ExtraTic>
SettingName=Резервные копии команд движения
SettingDescription=Автоматически делать бэкапы передаваемых по сети команд движения игроков.
SettingType=Boolean
Param=-extratic
Port=ZDoom,GZDoom
</ExtraTic>

<NetMode>
SettingName=Режим передачи данных
SettingDescription=Выберите режим передачи данных в сетевой игре.
SettingType=List
Item0=Peer to peer
Value0=0
Item1=Master/slave
Value1=1
Default=0
Param=-netmode
Port=ZDoom,GZDoom
</NetMode>

<TeamDamage>
SettingName=Friendly Fire
SettingDescription=Процент повреждений, который получают игроки от игроков своей команды
SettingType=TrackBar
Frequency=0.05
MinValue=0.0
MaxValue=1.0
Default=0.0
NotUseIfEqual=0.0
Param=+teamdamage
</TeamDamage>

<ForcePassword>
SettingName=Включить пароль сервера
SettingDescription=Запрашивать пароль сервера при подключении
SettingType=Boolean
Default=False
Param=+sv_forcepassword 1
FalseParam=+sv_forcepassword 0
Port=Skulltag
</ForcePassword>

<ServerPassword>
SettingName=Пароль сервера
SettingDescription=Пароль, который будет проверяться при подключении
SettingType=String
Default=
Param=+sv_password

DependsOn0=ForcePassword
DependCondition0=Equal
DependValue0=True
Port=Skulltag
</ServerPassword>

<ForceJoinPassword>
SettingName=Включить пароль игры
SettingDescription=Включить пароль для присоединения к игре
SettingType=Boolean
Default=False
Port=Skulltag
Param=+sv_forcejoinpassword 1
FalseParam=+sv_forcejoinpassword 0
</ForceJoinPassword>

<JoinPassword>
SettingName=Пароль игры
SettingDescription=Пароль, который будет проверяться при присоединении к игре
SettingType=String
Default=
Port=Skulltag
Param=+sv_joinpassword

DependsOn0=ForceJoinPassword
DependCondition0=Equal
DependValue0=True
</JoinPassword>